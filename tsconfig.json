{
  "compilerOptions": {
    "module": "es2015",
    "lib": ["es6"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": "./",
    "typeRoots": ["./types"],
    "moduleResolution": "node",
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.d.ts", "test/*.ts"]
}
